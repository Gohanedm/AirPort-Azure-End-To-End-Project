{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DfSpotifyAzProject"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"LS_AZSQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AZSQLDB'"
		},
		"LS_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_databricks'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stgspotifyazureproject.dfs.core.windows.net/"
		},
		"LS_AZSQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotityazureprojectserver.database.windows.net"
		},
		"LS_AZSQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "apotityazureprojectDB"
		},
		"LS_AZSQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminlogin"
		},
		"LS_HTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"LS_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1217-123423-ahwjffs2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DfSpotifyAzProject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"LS_AZSQLDB_password": {
						"value": "[parameters('LS_AZSQLDB_password')]"
					},
					"LS_databricks_accessToken": {
						"value": "[parameters('LS_databricks_accessToken')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"LS_AZSQLDB_properties_typeProperties_server": {
						"value": "[parameters('LS_AZSQLDB_properties_typeProperties_server')]"
					},
					"LS_AZSQLDB_properties_typeProperties_database": {
						"value": "[parameters('LS_AZSQLDB_properties_typeProperties_database')]"
					},
					"LS_AZSQLDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_AZSQLDB_properties_typeProperties_userName')]"
					},
					"LS_HTTP_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTP_properties_typeProperties_url')]"
					},
					"LS_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_databricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}